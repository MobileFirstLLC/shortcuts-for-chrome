{
  "name": "shortcuts-for-chrome",
  "version": "3.0.0",
  "license": "MIT",
  "author": {
    "name": "Mobile First>",
    "email": "hello@mobilefirst.me",
    "url": "https://mobilefirst.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MobileFirstLLC/shortcuts-for-chrome.git"
  },
  "description": "Menu of internal chrome links",
  "homepage": "https://chrome.google.com/webstore/detail/jnmekaomnicdcpgdndekkmojfomifjal",
  "scripts": {
    "start": "xt-build -e dev",
    "build": "xt-build -e prod",
    "docs": "xt-docs",
    "test": "xt-test",
    "clean": "xt-clean",
    "sync": "xt-sync -e -g"
  },
  "devDependencies": {
    "cws-publish": "^1.0.13",
    "extension-cli": "^0.9.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintIgnore": [
    "test/**/*"
  ],
  "xtbuild": {
    "js_bundles": [
      {
        "name": "background",
        "src": "./src/background/background.js"
      },
      {
        "name": "popup",
        "src": "./src/popup/**/*.js"
      }
    ]
  },
  "xtdocs": {
    "source": {
      "include": [
        "README.md",
        "src"
      ]
    },
    "opts": {
      "destination": "./pages",
      "tutorials": "docs",
      "template": "./node_modules/foodoc/template"
    },
    "templates": {
      "inlineNav": true,
      "search": false,
      "analytics": {
        "ua": "UA-129118591-7",
        "domain": "https://oss.mobilefirst.me/shortcuts-for-chrome"
      },
      "systemName": "Shortcuts for Chrome",
      "systemSummary": "Source code documentation",
      "systemColor": "#546E7A",
      "methodHeadingReturns": false,
      "stylesheets": [
        "data:text/css;base64,Zm9vdGVyewogICAgZGlzcGxheTpub25lIWltcG9ydGFudDsKfQ==",
        "https://raw.githubusercontent.com/MobileFirstLLC/shortcuts-for-chrome/master/docs/style.css"
      ],
      "navMembers": [
        {
          "kind": "namespace",
          "title": "Constants",
          "summary": "Application constants"
        },
        {
          "kind": "class",
          "title": "Classes",
          "summary": "All documented classes."
        },
        {
          "kind": "module",
          "title": "Modules",
          "summary": "All documented modules."
        },
        {
          "kind": "tutorial",
          "title": "Documents",
          "summary": "All other documents."
        }
      ]
    }
  }
}
