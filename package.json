{
  "name": "shortcuts-for-chrome",
  "version": "3.3.0",
  "license": "MIT",
  "author": {
    "name": "Mobile First",
    "email": "hello@mobilefirst.me",
    "url": "https://mobilefirst.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MobileFirstLLC/shortcuts-for-chrome.git"
  },
  "description": "Handy menu of Chrome URLs",
  "homepage": "https://chrome.google.com/webstore/detail/jnmekaomnicdcpgdndekkmojfomifjal",
  "scripts": {
    "start": "xt-build -e dev -w",
    "build": "xt-build -e prod",
    "docs": "xt-docs",
    "test": "xt-test",
    "clean": "xt-clean",
    "sync": "xt-sync -e",
    "translate": "node translate"
  },
  "devDependencies": {
    "csv-parser": "^3.0.0",
    "cws-publish": "^2.0.2",
    "extension-cli": "^0.11.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintIgnore": [
    "test/**/*"
  ],
  "standard-version": {
    "infile": "docs/CHANGELOG.md"
  },
  "xtbuild": {
    "js_bundles": [
      {
        "name": "background",
        "src": "./src/background/index.js"
      },
      {
        "name": "popup",
        "src": "./src/ui/index.js"
      }
    ],
    "scss_bundles": [
      {
        "src": [
          "./src/ui/theme.scss",
          "./src/ui/popup/popup.scss",
          "./src/**/*.scss"
        ],
        "name": "styles.css"
      }
    ],
    "locales_list": [
      "en",
      "fi"
    ]
  },
  "xtdocs": {
    "source": {
      "include": [
        "./.github/DOCS_INDEX.md",
        "src"
      ]
    },
    "opts": {
      "destination": "./pages",
      "tutorials": "docs",
      "template": "./node_modules/foodoc/template"
    },
    "templates": {
      "inlineNav": true,
      "search": false,
      "analytics": {
        "ua": "UA-129118591-7",
        "domain": "https://oss.mobilefirst.me/shortcuts-for-chrome"
      },
      "systemName": "Shortcuts for Chrome",
      "systemSummary": "Source code documentation",
      "systemColor": "#3D5AFE",
      "methodHeadingReturns": false,
      "stylesheets": [
        "data:text/css;base64,Zm9vdGVyewogICAgZGlzcGxheTpub25lIWltcG9ydGFudDsKfQ==",
        "data:text/css;base64,"
      ],
      "navMembers": [
        {
          "kind": "namespace",
          "title": "Constants",
          "summary": "Application constants"
        },
        {
          "kind": "module",
          "title": "Modules",
          "summary": "All documented modules."
        },
        {
          "kind": "tutorial",
          "title": "Documents",
          "summary": "All other documents."
        }
      ]
    }
  }
}
